"KeyInfo": {
	"EncryptedKey" : {
		"EncryptionMethod": {
			"Algorithm": "<%= keyEncryptionMethod %>",
			"DigestMethod" : {
				"Algorithm": "http://tiagomistral.github.io/SecJSON#sha1"
			}
		},
		"KeyInfo": {
			"RetrievalMethod": "<%- encryptionPublicCert %>"
		},
		"CipherData": {
			"CipherValue": "<%= encryptedKey %>"
		}
	}
}